def checkString(str):
    flag_1 = False
    flag_2 = False
    for i in str:
        if i.isalpha():
            flag_1 = True
        if i.isdigit():
            flag_2 = True
            
    return flag_1 and flag_2
    
print(checkString('As you can see, this has both letters and a digit, which is 7.'))
print(checkString('This does not have a digit.'))
print(checkString('01001001 01100110 00100000 01111001 01101111 01110101 00100000 01101011 01101110 01101111 01110111 00100000 01100010 01101001 01101110 01100001 01110010 01111001 00101100 00100000 01111001 01101111 01110101 00100000 01110111 01101001 01101100 01101100 00100000 01101011 01101110 01101111 01110111 00100000 01110100 01101000 01100001 01110100 00100000 01110100 01101000 01101001 01110011 00100000 01100100 01101111 01100101 01110011 00100000 01101110 01101111 01110100 00100000 01101000 01100001 01110110 01100101 00100000 01100001 00100000 01101100 01100101 01110100 01110100 01100101 01110010 00101110 00100000 01000001 01100011 01110100 01110101 01100001 01101100 01101100 01111001 00100000 01111001 01101111 01110101 00100000 01100100 01101111 01101110 00100111 01110100 00100000 01101110 01100101 01100101 01100100 00100000 01100010 01101001 01101110 01100001 01110010 01111001 00100000 01100110 01101111 01110010 00100000 01110100 01101000 01100001 01110100 00101110 00100000 01000001 01100100 01101001 01101111 01110011 00100001'))
