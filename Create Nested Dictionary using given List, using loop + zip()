test_dict = {'Gfg' : 4, 'is' : 5, 'best' : 9}
test_list = [8, 3, 2]
print("The original Dictionary : " + str(test_dict))
print("The original List : " + str(test_list))

result = {}

for key, ele in zip(test_list, test_dict.items()):
    result[key] = dict([ele])
    
print("The mapped Dictionary : " + str(result))
