from operator import mod
tuple1 = (10, 4, 5, 6)
tuple2 = (5, 6, 7, 5)

print("The original tuple 1 is : ", tuple1)
print("The original tuple 2 is : ", tuple2)

result = []
for i in range(0, len(tuple1)):
    result.append(tuple1[i] % tuple2[i])

result = tuple(result)
print("The Modulus Tuple is : ", result)
